{
    "testTitle": "Invoice Test Scenarios - Scenario 1",
    "headless": false,
    "starturl": "new-order",    
    "testData" : {      
      "products":["product1Name"]
    },
    "testCases": [
      {
        "keyname": "order",
        "input": {"products":["product1Name"]},
        "output": {"orderId": "order1Id"},
        "eta": 2,
        "products": [
          {
            "id": "product1Name",
            "quantity": 1500,
            "unitPrice": 379
          }
        ]
      },
      {
        "keyname": "delivery",        
        "input": {"orderId":"order1Id"},
        "bdnDate": 1,
        "buyerQuantity": "Bdn Quantity",
        "sellerQuantity": "Bdn Quantity",  
        "products": [
          {
            "id": "product1Name",
            "quantity": 1500
          }
        ]
      },
      {
        "keyname": "treasury",
        "input": {"orderId":"order1Id"},
        "rows": [ 
          {
            "Calculated Amount": "568500"
          }
        ]
      },
      {
        "keyname": "completeView",
        "input": {"orderId":"order1Id"},
        "rows": [ 
          {
            "Order Product Amount": "568500"
          }
        ]
      },
      {
        "keyname":"invoice",
          "action": "provisional",
          "input": {"orderId":"order1Id"},
          "output": {"invoiceId": "invoice1Id"},          
          "invoiceStatusAfterSave": ["New","Discrepancy"],
          "invoiceStatusAfterSubmit": ["New", "Discrepancy", "WaitingForApproval"],
          "invoiceStatusAfterApprove": ["Approved"],
          "testRevert": false,
          "testOrderStatus": false,
          "paymentDate": 2,
          "provisionalData" :{ 
              "products": [
                {
                  "id": "product1Name",
                  "quantity": 1499,
                  "rate": 380
                }
              ]
            }
        },
        {
          "keyname": "treasury",
          "input": {"orderId":"order1Id"},
          "rows": [ 
            {
              "Calculated Amount": "569620",
              "Invoice Amount": "569620",
              "Invoice Type": "Provisional Invoice"
            }
          ]
        },
        {
          "keyname": "completeView",
          "input": {"orderId":"order1Id"},
          "rows": [ 
            {
              "Order Product Amount": "568121",
              "Invoice Product Amount": "569620"
            }
          ]
        },
        {
          "keyname":"invoice",
            "action": "finalAfterProvisional",
            "input": {"orderId": "order1Id", "invoiceId":"invoice1Id"},
            "invoiceStatusAfterSave": ["New","Discrepancy"],
            "invoiceStatusAfterSubmit": ["New","Discrepancy", "WaitingForApproval"],
            "invoiceStatusAfterApprove": ["Approved"],
            "testRevert": false,
            "testOrderStatus": false,
            "paymentDate": 2,         
            "finalData": {
              "products": [
                {
                  "id": "product1Name",
                  "quantity": 1499,
                  "rate": 373.65
                }
              ]
            }
          },
          {
          "keyname": "treasury",
          "input": {"orderId":"order1Id"},
          "rows": [ 
            {
              "Calculated Amount": "569620",
              "Invoice Amount": "569620",
              "Invoice Type": "Provisional Invoice"
            },
            {
              "Calculated Amount": "-9518",
              "Invoice Amount": "-9518",                        
              "Invoice Type": "Final Invoice"
            }
          ]
        },
        {
          "keyname": "completeView",
          "input": {"orderId":"order1Id"},
          "rows": [ 
            {
              "Order Product Amount": "568121",
              "Invoice Product Amount": "560101"
            }
          ]
        } 
  ]
}



  